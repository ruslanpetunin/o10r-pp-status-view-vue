{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "declaration": true,
    "declarationDir": "dist",
    "emitDeclarationOnly": true,
    "outDir": "dist",
    "strict": true,
    "jsx": "preserve",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
    "paths": {
      "o10r-pp-core": ["./../o10r-pp-core/src", "o10r-pp-core"],
      "o10r-pp-payment-method": ["./../o10r-pp-payment-method/src", "o10r-pp-payment-method"],
      "orchestrator-pp-payment-form": ["./../orchestrator-pp-payment-form/src", "orchestrator-pp-payment-form"],
      "o10r-pp-ui-kit-vue": ["./../o10r-pp-ui-kit-vue/src", "o10r-pp-ui-kit-vue"]
    }
  },
  "include": ["src"]
}